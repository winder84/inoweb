{% block content %}
	<ul class="servicesUl">
		{% for key,service in services %}
			{% if key < 3 %}
				<li>
					<img class="serviceImg" src="{% path service.Media, 'reference' %}" />
					<div class="serviceBody">
						<h3 class="serviceTitle">
							{{ service.title|upper }}
						</h3>
						<div class="serviceDelimer"></div>
						<p class="serviceText">
							{{ service.text }}
						</p>
					</div>
				</li>
			{% endif %}
		{% endfor %}
	</ul>
{% endblock %}